[remotecontrol]
telnet=1
telnetport=2121
zmqctrl=1
zmqctrlendpoint=tcp://127.0.0.1:9400

[log]
syslog=0
filelog=0
filename=/dev/stderr

[input]
;transport=file
;source=/dev/stdin
;transport=zeromq
transport=zeromq
source=tcp://dab.audiostore.co.za:9100
;loop=0
max_frames_queued=400

[modulator]
gainmode=var
normalise_variance=4
mode=1

dac_clk_rate=0
digital_gain=0.8

rate=2048000

[firfilter]
enabled=1
filtertapsfile=/home/frank/dab/ODR-DabMod/doc/fir-filter/simplefiltertaps.txt

[output]
output=soapysdr

[fileoutput]
filename=/dev/null

[soapyoutput]
device=LimeSDR-Mini
master_clock_rate=32768000
txgain=40
;frequency=234208000
channel=13C
tx_antenna=Auto
rx_antenna=Auto

[delaymanagement]
synchronous=0
offset=2.0
